class Parent
{
int a = 10;
void display()
{
System.out.println("parent");
}
}
class Child extends Parent
{
int a = 20;
void display()
{
System.out.println(a);
System.out.println("child");
System.out.println(super.a);
super.display();
}
}
class Super
{
public static void main(String [] args)
{
Child obj = new Child();
obj.display();
}
}
